{
  "name": "@sentry/aws-serverless",
  "version": "8.0.0",
  "description": "Official Sentry SDK for AWS Lambda and AWS Serverless Environments",
  "repository": "git://github.com/getsentry/sentry-javascript.git",
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/serverless",
  "author": "Sentry",
  "license": "MIT",
  "engines": {
    "node": ">=14.18"
  },
  "files": [
    "cjs",
    "esm",
    "types",
    "types-ts3.8",
    "import-hook.mjs",
    "loader-hook.mjs"
  ],
  "main": "cjs/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./cjs/index.js"
      }
    },
    "./import": {
      "import": {
        "default": "./build/import-hook.mjs"
      }
    },
    "./loader": {
      "import": {
        "default": "./build/loader-hook.mjs"
      }
    }
  },
  "typesVersions": {
    "<4.9": {
      "types/index.d.ts": [
        "types-ts3.8/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@opentelemetry/instrumentation-aws-lambda": "0.40.0",
    "@opentelemetry/instrumentation-aws-sdk": "0.40.0",
    "@sentry/core": "8.0.0",
    "@sentry/node": "8.0.0",
    "@sentry/types": "8.0.0",
    "@sentry/utils": "8.0.0",
    "@types/aws-lambda": "^8.10.62"
  },
  "devDependencies": {
    "@types/node": "^14.18.0"
  },
  "sideEffects": false,
  "nx": {
    "targets": {
      "build:bundle": {
        "dependsOn": [
          "build:transpile",
          "build:types"
        ],
        "outputs": [
          "{projectRoot}/build/aws"
        ]
      }
    }
  }
}